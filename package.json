{
  "name": "nextjs-cms-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "PRISMA_HIDE_UPDATE_MESSAGE=1 node scripts/prisma-generate.mjs",
    "dev": "next dev",
    "build": "PRISMA_HIDE_UPDATE_MESSAGE=1 node scripts/prisma-generate.mjs && NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --check .",
    "prisma": "pnpm exec prisma",
    "prisma:generate": "node scripts/prisma-generate.mjs",
    "postinstall": "PRISMA_HIDE_UPDATE_MESSAGE=1 node scripts/prisma-generate.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@clerk/nextjs": "^6.37.3",
    "@clerk/themes": "^2.4.51",
    "@clerk/types": "^4.101.14",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "prisma": "^6.19.2",
    "quill": "^2.0.3",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^4",
    "react-use": "^17.6.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ],
    "allowedDeprecatedVersions": {
      "lodash.isequal": "*"
    }
  }
}
